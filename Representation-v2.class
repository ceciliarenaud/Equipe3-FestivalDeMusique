<?php
namespace modele\metier;

class Representation {
    private $id;
    private $date;
    private $lieu;
    private $groupe;
    function __construct($id, $date, $groupe, $lieu) {
        $this->id=$id;
        $this->date = $date;
        $this->lieu = $lieu;
        $this->groupe = $groupe;
    }
    function getId() {
        return $this->id;
    }
    function getDate() {
        return $this->date;
    }
    function getLieu() {
        return $this->lieu;
    }
    function getGroupe() {
        return $this->groupe;
    }
    function setId($id) {
        $this->id = $id;
       }
    function setDate($date) {
        $this->date = $date;
    }
    function setLieu($lieu) {
        $this->lieu = $lieu;
    }
    function setGroupe($groupe) {
        $this->groupe = $groupe;
    
    }
}